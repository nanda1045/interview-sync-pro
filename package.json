{
  "name": "interviewsync-pro",
  "version": "1.0.0",
  "description": "LeetCode-style collaborative mock interview platform",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=server\" \"npm run dev --workspace=client\"",
    "build": "npm run build --workspace=server && npm run build --workspace=client",
    "install:all": "npm install && npm install --workspace=client && npm install --workspace=server"
  },
  "keywords": [
    "interview",
    "collaborative",
    "coding",
    "leetcode",
    "realtime"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
